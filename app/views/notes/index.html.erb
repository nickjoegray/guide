<div class="btn new-note">
  <h4><%= link_to "Create New Note", :action => "new" %></h4>
</div>
<br>
<br>
<div class="total-notes">
<h2>All of your Notes:</h2>
<div class="all-notes">
<ul>
  <% @notes.each do |note| %>
  <li><span class="larger"><%= link_to note.title, note_path(note) %></span></li>
<% end %>
</ul>
</div>
</div>
